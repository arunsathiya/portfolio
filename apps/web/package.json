{
  "name": "@portfolio/web",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "notion-to-md": "tsx src/scripts/notion-to-md.ts",
    "changes-on-notion": "tsx src/scripts/changes-on-notion.ts",
    "notion": "tsx src/scripts/notion-to-md.ts && tsx src/scripts/changes-on-notion.ts",
    "list-buckets": "tsx src/utils/s3/listBuckets.ts",
    "list-objects": "tsx src/utils/s3/listObjects.ts",
    "list-objects-signed": "tsx src/utils/s3/listObjectsSigned.ts",
    "clean-and-dev": "rm -rf node_modules && bun install && bun run format:write && bun run dev",
    "notion-format": "bun run notion-to-md && bun run format:write"
  },
  "dependencies": {
    "@11ty/is-land": "^5.0.1",
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.6.0",
    "@astrojs/markdown-remark": "^6.3.3",
    "@astrojs/mdx": "^4.3.1",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vercel": "^9.0.3",
    "@aws-sdk/client-s3": "^3.666.0",
    "@aws-sdk/lib-storage": "^3.666.0",
    "@aws-sdk/s3-request-presigner": "^3.666.0",
    "@fontsource/ibm-plex-mono": "^5.1.1",
    "@fontsource/ibm-plex-sans": "^5.1.0",
    "@notionhq/client": "2.3.0",
    "@rollup/rollup-linux-x64-gnu": "^4.45.1",
    "@supabase/supabase-js": "^2.52.0",
    "@zachleat/snow-fall": "^1.0.3",
    "astro": "^5.12.0",
    "astro-embed": "^0.12.0",
    "cssesc": "./overrides/cssesc",
    "dotenv": "^17.2.3",
    "notion-to-md": "^3.1.1",
    "sharp": "^0.34.5",
    "sort-keys": "^6.0.0",
    "tailwindcss": "3",
    "typescript": "^5.6.2",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "tsx": "^4.19.1",
    "vite": "5"
  },
  "resolutions": {
    "cssesc": "./overrides/cssesc"
  },
  "packageManager": "bun"
}
